package com.team02.u25.ejercicio1.service;

import java.util.List;

import com.team02.u25.ejercicio1.dto.Empleado;


public interface IEmpleadoService {

	//Metodos del CRUD
	public List<Empleado> listarDepartamento(); 
	
	public Empleado guardarEmpleado(Empleado departamento);	
	
	public Empleado empleado(Long id); 
	
	public Empleado actualizarEmpleado(Empleado departamento); 
	
	public void eliminarDepartamento(Long id);
}
